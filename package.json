{
  "name": "backend-engineer-test",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/index.ts --watch",
    "dev": "tsx watch src/index.ts",
    "db:generate": "bunx drizzle-kit generate --name",
    "db:migrate": "bunx drizzle-kit push",
    "run-docker": "docker-compose up -d --build",
    "lint": "eslint .",
    "format": "prettier --write .",
